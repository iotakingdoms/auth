    "eslint": "8.31.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.2.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-puppeteer": "6.2.0",
    "puppeteer": "19.4.1",
    "standard-version": "9.5.0",
    "ts-jest": "29.0.3",
    "typedoc": "0.23.23",
    "typescript": "4.9.4"
  }
}
