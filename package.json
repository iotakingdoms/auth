    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@types/jest": "29.2.5",
    "@types/jest-environment-puppeteer": "5.0.3",
    "@types/oidc-provider": "7.14.0",
    "@types/puppeteer": "5.4.7",
